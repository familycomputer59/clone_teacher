<%= bootstrap_form_for(@rental) do |f| %>
  <%= f.date_select :rental_date %>
  <%= f.date_select :due_date %>
  <b>Labware details</b>
  <% @rental_details.each do |rental_detail| %>
    <%= render 'rentals/rental_box_row', labware: rental_detail.labware, rental_detail: rental_detail, show_total: true %>
  <% end %>
  <%= f.primary "OK" %>
<% end %>